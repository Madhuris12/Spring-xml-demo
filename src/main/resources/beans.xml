<?xml version = "1.0" encoding = "UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

      <bean id="movie4" autowire="byType" class="com.stackroute.spring.beans.Movie">
        <property name="movieName" value="Prabhas"></property>
        <property name="director" value="Bahubali"></property>
    </bean>



    <bean id="movie5" autowire="byName" class="com.stackroute.spring.beans.Movie">
        <property name="movieName" value="Prabhas"></property>
        <property name="director" value="Bahubali"></property>
    </bean>

    <bean id="actor" class="com.stackroute.spring.beans.Actor">
        <property name="name" value="Prabhas"></property>
        <property name="age" value="35"></property>
        <property name="gender" value="male"></property>
    </bean>



</beans>
